{
    "name": "dmitrychurkin/keap",
    "description": "Keap REST API Client for Laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "keap",
        "infusionsoft",
        "rest",
        "api",
        "client"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Churkin",
            "email": "dmitrychurkin1988@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": ">=9.0",
        "keap/keap-sdk": "^1.0.40"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0",
        "laravel/pint": "^1.27"
    },
    "autoload": {
        "psr-4": {
            "DmitryChurkin\\Keap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DmitryChurkin\\Keap\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DmitryChurkin\\Keap\\KeapServiceProvider"
            ],
            "aliases": {
                "Keap": "DmitryChurkin\\Keap\\Facades\\Keap"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    }
}
